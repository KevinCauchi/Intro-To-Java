
public class Fahrenheit {
public static void main(String[]arsgs) {
	
	for(int i =0;i<=100;i++) {
		int celsius=(((i-32)*5)/9);
		System.out.println("Fahrenheit: "+ i+ "\tCelsius: "+ celsius);
	}
	
	
}
	
}


